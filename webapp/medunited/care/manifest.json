{
	"_version": "1.21.0",
	"sap.app": {
		"id": "medunited.care",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"dataSources": {
			"fhir": {
				"uri": "http://localhost:8080/fhir",
				"type": "FHIR"
			},
			"app-state": {
				"uri": "../../resources/local/state.json",
				"type": "JSON"
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "medunited.care.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.98.0",
			"libs": {
				"sap.ui.core": {},
				"sap.tnt": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"": {
				"type": "sap.fhir.model.r4.FHIRModel",
				"dataSource": "fhir",
				"settings": {
					"groupProperties": {
						"patientDetails": {
							"submit": "Transaction"
						},
						"practitionerDetails": {
							"submit": "Transaction"
						},
						"organizationDetails": {
							"submit": "Transaction"
						}
					},
					"defaultQueryParameters": {
						"_total": "accurate"
					}
				}
			},
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "medunited.care.i18n.i18n"
				}
			},
			"appState": {
				"dataSource": "app-state",
				"type": "sap.ui.model.json.JSONModel"
			}
		},
		"resources": {
			"js": [
				{
				"uri": "./lib/zxing-0.19.1.min.js"
				}
			],
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "medunited.care.view",
				"controlId": "fcl",
				"transition": "slide"
			},
			"routes": [
				{
					"pattern": "",
					"name": "home",
					"target": [
						"patient-master"
					]
				},
				{
					"pattern": "patient/action/add",
					"name": "patient-add",
					"target": [
						"patient-master",
						"patient-detail"
					]
				},
				{
					"pattern": "patient/:layout:",
					"name": "patient-master",
					"target": [
						"patient-master",
						"patient-detail"
					]
				},
				{
					"pattern": "patient/search/:query:",
					"name": "patient-search",
					"target": [
						"patient-master",
						"patient-detail"
					]
				},
				{
					"pattern": "patient/{patient}/{layout}",
					"name": "patient-detail",
					"target": [
						"patient-master",
						"patient-detail"
					]
				},
				{
					"pattern": "practitioner/action/add",
					"name": "practitioner-add",
					"target": [
						"practitioner-master",
						"practitioner-detail"
					]
				},
				{
					"pattern": "practitioner/:layout:",
					"name": "practitioner-master",
					"target": [
						"practitioner-master",
						"practitioner-detail"
					]
				},
				{
					"pattern": "practitioner/search/:query:",
					"name": "practitioner-search",
					"target": [
						"practitioner-master",
						"practitioner-detail"
					]
				},
				{
					"pattern": "practitioner/{practitioner}/{layout}",
					"name": "practitioner-detail",
					"target": [
						"practitioner-master",
						"practitioner-detail"
					]
				},
				{
					"pattern": "medication/:layout:",
					"name": "medication-master",
					"target": [
						"medication-master"
					]
				},
				{
					"pattern": "organization/action/add",
					"name": "organization-add",
					"target": [
						"organization-master",
						"organization-detail"
					]
				},
				{
					"pattern": "organization/:layout:",
					"name": "organization-master",
					"target": [
						"organization-master",
						"organization-detail"
					]
				},
				{
					"pattern": "organization/search/:query:",
					"name": "organization-search",
					"target": [
						"organization-master",
						"organization-detail"
					]
				},
				{
					"pattern": "organization/{organization}/{layout}",
					"name": "organization-detail",
					"target": [
						"organization-master",
						"organization-detail"
					]
				}
			],
			"targets": {
				"patient-master": {
					"viewName": "patient/Master",
					"controlAggregation": "beginColumnPages",
					"clearControlAggregation": true
				},
				"patient-detail": {
					"viewName": "patient/Detail",
					"controlAggregation": "midColumnPages",
					"clearControlAggregation": true
				},
				"practitioner-master": {
					"viewName": "practitioner/Master",
					"controlAggregation": "beginColumnPages",
					"clearControlAggregation": true
				},
				"practitioner-detail": {
					"viewName": "practitioner/Detail",
					"controlAggregation": "midColumnPages",
					"clearControlAggregation": true
				},
				"medication-master": {
					"viewName": "medication/Master",
					"controlAggregation": "beginColumnPages",
					"clearControlAggregation": true
				},
				"organization-master": {
					"viewName": "organization/Master",
					"controlAggregation": "beginColumnPages",
					"clearControlAggregation": true
				},
				"organization-detail": {
					"viewName": "organization/Detail",
					"controlAggregation": "midColumnPages",
					"clearControlAggregation": true
				}
			}
		}
	}
}