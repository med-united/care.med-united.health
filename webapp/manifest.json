{
	"_version": "1.21.0",
	"sap.app": {
		"id": "webapp",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"dataSources": {
			"fhir": {
				"uri": "http://localhost:8080/fhir",
				"type": "FHIR"
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "webapp.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.98.0",
			"libs": {
				"sap.ui.core": {},
				"sap.tnt": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"": {
				"type": "sap.fhir.model.r4.FHIRModel",
				"dataSource": "fhir",
				"settings" : {
					"groupProperties": {
						"patientDetails": {
							"submit": "Batch"
						}
					},
					"defaultQueryParameters": {
						"_total": "accurate"
					}
				}
			},
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
				  "bundleName": "webapp.i18n.i18n"
				}
			  }
		},
		"config": {
			"invoiceLocal": "/Invoices.json"
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "webapp.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "flip"
			},
			"routes": [
				{
					"pattern": "/pharmacy",
					"name": "pharmacy",
					"target": "pharmacy"
				},
				{
					"pattern": "",
					"name": "overview",
					"target": "overview"
				}
			],
			"targets": {
				"pharmacy": {
					"viewName": "Pharmacy",
					"viewId": "pharmacy",
					"viewLevel": 1
				},
				"overview": {
					"viewName": "Overview",
					"viewId": "overview",
					"viewLevel": 1
				}
			}
		}
	}
}